<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include layout="@layout/include_title_main"/>
    <ViewAnimator
        android:id="@+id/mva"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="台站代码"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_tai"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="设备ID"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_seid"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_1"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="设备名称"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_name"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_2"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="设备型号"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_type"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_3"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="生产日期"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_date"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_4"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="联系方式"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_phone"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:id="@+id/ll_11"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_5"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="通讯地址"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_address"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>

                <LinearLayout
                    android:id="@+id/ll_12"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_6"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="生产厂商"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_company"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:id="@+id/ll_13"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_7"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="数采软件版本号"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_banbenno"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/ll_8"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_8"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="数采软件版本号"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_8_"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/ll_9"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_9"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="数采软件版本号"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_9_"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/ll_10"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_10"
                        android:drawablePadding="@dimen/margin_5"
                        android:drawableLeft="@mipmap/table"
                        android:text="数采软件版本号"
                        android:textSize="16sp"
                        android:textColor="@color/text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_10_"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:text="--"
                        android:textSize="14sp"
                        android:textColor="@color/text"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <include layout="@layout/include_line_one"/>
            </LinearLayout>
        </ScrollView>
        <include layout="@layout/include_nodata"/>
    </ViewAnimator>


</LinearLayout>